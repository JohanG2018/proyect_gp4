<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="motoman_gp4" params="prefix">
    <xacro:include filename="$(find motoman_resources)/urdf/common_materials.xacro"/>

    <!-- link list -->
    <link name="${prefix}base_link">
      <visual>
        <geometry>
          <mesh filename="package://gp4_description/meshes/gp4/visual/base_link.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://gp4_description/meshes/gp4/collision/base_link.stl"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="23.085357"/>
        <origin rpy="0.0 0.0 0.0" xyz="0.007579 0.000017 0.180670"/>
        <inertia ixx="0.343576" ixy="-0.000062" ixz="-0.002079" iyy="0.336217" iyz="-0.000014" izz="0.334050"/>
       </inertial>
    </link>
    <link name="${prefix}link_1_s">
      <visual>
        <geometry>
          <mesh filename="package://gp4_description/meshes/gp4/visual/link_1_s.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://gp4_description/meshes/gp4/collision/link_1_s.stl"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="45.357763"/>
        <origin rpy="0.0 0.0 0.0" xyz="0.085479 -0.002547 -0.040488"/>
        <inertia ixx="0.839141" ixy="0.022637" ixz="0.162457" iyy="1.030146" iyz="0.027416" izz="1.064698"/>
      </inertial>
    </link>
    <link name="${prefix}link_2_l">
      <visual>
        <geometry>
          <mesh filename="package://gp4_description/meshes/gp4/visual/link_2_l.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://gp4_description/meshes/gp4/collision/link_2_l.stl"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="51.266495"/>
        <origin rpy="0.0 0.0 0.0" xyz="-0.000002 0.264164 0.347704"/>
        <inertia ixx="5.272800" ixy="0.000010" ixz="0.000017" iyy="5.536809" iyz="-0.008261" izz="0.486191"/>
      </inertial>
    </link>
    <link name="${prefix}link_3_u">
      <visual>
        <geometry>
          <mesh filename="package://gp4_description/meshes/gp4/visual/link_3_u.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://gp4_description/meshes/gp4/collision/link_3_u.stl"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="19.754401"/>
        <origin rpy="0.0 0.0 0.0" xyz="-0.000255 0.016478 -0.003819"/>
        <inertia ixx="0.249388" ixy="-0.004901" ixz="0.004825" iyy="0.211780" iyz="0.000574" izz="0.238488"/>
      </inertial>
    </link>
    <link name="${prefix}link_4_r">
      <visual>
        <geometry>
          <mesh filename="package://gp4_description/meshes/gp4/visual/link_4_r.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://gp4_description/meshes/gp4/collision/link_4_r.stl"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="15.287896"/>
        <origin rpy="0.0 0.0 0.0" xyz="-0.015515 0.000164 0.340348"/>
        <inertia ixx="0.331593" ixy="0.000055" ixz="-0.010688" iyy="0.320895" iyz="0.000058" izz="0.086557"/>
      </inertial>
    </link>
    <link name="${prefix}link_5_b">
      <visual>
        <geometry>
          <mesh filename="package://gp4_description/meshes/gp4/visual/link_5_b.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://gp4_description/meshes/gp4/collision/link_5_b.stl"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.548088"/>
        <origin rpy="0.0 0.0 0.0" xyz="0.000000 -0.000347 0.023671"/>
        <inertia ixx="0.000876" ixy="0.000000" ixz="0.000000" iyy="0.000889" iyz="0.000005" izz="0.000412"/>
       </inertial>
    </link>
    <link name="${prefix}link_6_t">
      <visual>
        <geometry>
          <mesh filename="package://gp4_description/meshes/gp4/visual/link_6_t.dae"/>
        </geometry>
        <xacro:material_yaskawa_silver/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://gp4_description/meshes/gp4/collision/link_6_t.stl"/>
        </geometry>
      </collision>
      <inertial>
        <mass value="0.038484"/>
        <origin rpy="0.0 0.0 0.0" xyz="-0.000245 0.000000 -0.007626"/>
        <inertia ixx="0.000011" ixy="0.000000" ixz="0.000000" iyy="0.000011" iyz="0.000000" izz="0.000021"/>
       </inertial>
    </link>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_1_s" type="revolute">
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link_1_s"/>
      <origin xyz="0 0 0.330" rpy="0 0 0" />
      <axis xyz="0 0 1" />
      <limit lower="${radians(-170)}" upper="${radians(170)}" effort="220.5" velocity="${radians(465)}"/>
    </joint>
    <joint name="${prefix}joint_2_l" type="revolute">
      <parent link="${prefix}link_1_s"/>
      <child link="${prefix}link_2_l"/>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <axis xyz="0 1 0" />
      <limit lower="${radians(-110)}" upper="${radians(130)}" effort="305.76" velocity="${radians(465)}"/>
    </joint>
    <joint name="${prefix}joint_3_u" type="revolute">
      <parent link="${prefix}link_2_l"/>
      <child link="${prefix}link_3_u"/>
      <origin xyz="0 0 0.260" rpy="0 0 0" />
      <axis xyz="0 -1 0" />
      <limit lower="${radians(-65)}" upper="${radians(200)}" effort="95.55" velocity="${radians(525)}"/>
    </joint>
    <joint name="${prefix}joint_4_r" type="revolute">
      <parent link="${prefix}link_3_u"/>
      <child link="${prefix}link_4_r"/>
      <origin xyz="0.290 0 0.015" rpy="0 0 0" />
      <axis xyz="-1 0 0" />
      <limit lower="${radians(-200)}" upper="${radians(200)}" effort="48.02" velocity="${radians(565)}"/>
    </joint>
    <joint name="${prefix}joint_5_b" type="revolute">
      <parent link="${prefix}link_4_r"/>
      <child link="${prefix}link_5_b"/>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <axis xyz="0 -1 0" />
      <limit lower="${radians(-123)}" upper="${radians(123)}" effort="38.42" velocity="${radians(565)}"/>
    </joint>
    <joint name="${prefix}joint_6_t" type="revolute">
      <parent link="${prefix}link_5_b"/>
      <child link="${prefix}link_6_t"/>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <axis xyz="-1 0 0" />
      <limit lower="${radians(-455)}" upper="${radians(455)}" effort="16.46" velocity="${radians(1000)}"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS-Industrial 'flange' frame: attachment point for EEF models -->
    <link name="${prefix}flange"/>
    <joint name="${prefix}joint_6_t-flange" type="fixed">
      <origin xyz="0.072 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_6_t"/>
      <child link="${prefix}flange"/>
    </joint>

    <!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
    <link name="${prefix}tool0"/>
    <joint name="${prefix}flange-tool0" type="fixed">
      <origin xyz="0 0 0" rpy="${radians(180)} ${radians(-90)} 0"/>
      <parent link="${prefix}flange"/>
      <child link="${prefix}tool0"/>
    </joint>

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0.330" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>
  </xacro:macro>
</robot>
