<!--
  Manipulator specific version of 'robot_interface_streaming.launch'.

  Defaults provided for GP4:
   - 6 joints

  Usage:
    robot_interface_streaming_gp4.launch robot_ip:=<value> controller:=<yrc1000>
-->
<launch>
  <arg name="robot_ip" value="127.0.0.1"/>

  <!-- controller: Controller name (yrc1000) -->
  <arg name="controller" value="yrc1000"/>

  <rosparam command="load" file="$(find gp4_description)/config/joint_names_gp4.yaml" />

  <include file="$(find motoman_driver)/launch/robot_interface_streaming_$(arg controller).launch">
    <arg name="robot_ip" value="$(arg robot_ip)" />
  </include>
</launch>
